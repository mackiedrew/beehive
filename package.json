{
  "name": "beehive",
  "private": false,
  "license": "MIT",
  "repository": "http://github.com/mackiedrew/beehive/",
  "scripts": {
    "start": "MONGO_URL=mongodb://localhost:27017/beehive meteor run",
    "flow": "./node_modules/.bin/flow check",
    "flow:coverage": "./node_modules/.bin/flow-coverage-report",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "lint:check": "./node_modules/.bin/eslint --print-config .eslintrc.js | ./node_modules/.bin/eslint-config-prettier-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepush": "npm run lint && npm test"
  },
  "dependencies": {
    "babel-runtime": "6.25.0",
    "bootstrap": "4.0.0-alpha.6",
    "meteor-node-stubs": "0.2.4",
    "react": "15.6.1",
    "react-addons-css-transition-group": "15.6.0",
    "react-addons-transition-group": "15.6.0",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.2",
    "react-transition-group": "1.1.2",
    "reactstrap": "4.8.0",
    "redux": "3.7.2",
    "redux-saga": "0.15.6",
    "reselect": "3.0.1",
    "simpl-schema": "0.3.2",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.2.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "1.6.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "eslint": "4.3.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "flow-bin": "0.51.1",
    "flow-coverage-report": "0.3.0",
    "flow-typed": "2.1.5",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "jest-enzyme": "^4.0.0",
    "jest-flow-transform": "1.0.1",
    "jest-meteor-stubs": "1.5.1",
    "meteor-node-stubs": "0.2.11",
    "prettier": "1.5.3",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "3.4.0",
    "redux-saga-tester": "1.0.367"
  },
  "babel": {
    "presets": [
      "env",
      "flow",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-regenerator"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "flow-coverage-report": {
    "includeGlob": [
      "imports/**/*.js",
      "imports/**/*.jsx"
    ],
    "type": [
      "text",
      "html",
      "json"
    ],
    "outputDir": ".flow-coverage",
    "threshold": 40
  }
}
